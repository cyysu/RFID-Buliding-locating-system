<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC	
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
	
<struts>
	<package name="control" extends="struts-default">
		<action name="Action" class="control.Action" method="Action">
			<result name="Success">/welcome.jsp</result>
		</action>
		<action name="ManualAction" class="control.Action" method="ManualAction">
			<result name="Success">/welcome.jsp</result>
		</action>
		<action name="newestPos" class="control.Action" method="newestPos">
			<result name="Success">newestPos.jsp</result>
		</action>
		<action name="Event" class="control.Action" method="Event">
			<result name="Success">event.jsp</result>
		</action>
		<action name="selectbyemp" class="control.Action" method="selectbyemp">
			<result name="Success">selectbyemp.jsp</result>
		</action>
		<action name="selectbypos" class="control.Action" method="selectbypos">
			<result name="Success">selectbypos.jsp</result>
		</action>
		<action name="selectbydate" class="control.Action" method="selectbydate">
			<result name="Success">selectbydate.jsp</result>
		</action>
	</package>
</struts>